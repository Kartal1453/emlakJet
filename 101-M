function haritaChange() {

document.querySelector(".styles_showOnMap__2NEQd > img").insertAdjacentHTML('afterend', '<img class="round-keyboard_arrow_right-24px-1-copy-4" style="width: 7px;height: 12px;margin-right: 1.7px;margin-left: auto;object-fit: contain;" src="https://i.ibb.co/PQzcnjz/round-keyboard-arrow-right-24-px-1-copy-4.png" >');
document.querySelector(".styles_showOnMap__2NEQd > p").insertAdjacentHTML('afterend', '<img class="newTest101" style="margin: 9px 9px 9px 0px;width: 70px;height: 30px;" src="https://i.ibb.co/7RsLSPW/group-5.png" >');
document.querySelectorAll(".styles_showOnMap__2NEQd > img")[1].remove();
document.querySelector(".styles_showOnMap__2NEQd").style.justifyContent = "start";
document.querySelector(".styles_showOnMap__2NEQd > p").style.marginTop = "12px";
document.querySelector(".styles_showOnMap__2NEQd > p").style.marginLeft = "3px";
document.querySelector(".styles_showOnMap__2NEQd > img").parentNode.insertBefore(document.querySelector(".styles_showOnMap__2NEQd > img"), document.querySelector(".styles_showOnMap__2NEQd > p"));



}


haritaChange()
 var dom_observer = new MutationObserver(function (mutation) {
   
   if(!document.querySelector(".newTest101")){
     haritaChange();}
  });
  var container = document.body;
  var configs = { attributes: false, childList: true, characterData: false };
  dom_observer.observe(container, configs);
  
  
